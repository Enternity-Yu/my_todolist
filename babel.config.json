{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		],
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				"useESModules": false,
				"regenerator": true,
				"version": "7.0.0-beta.0"
			}
		]
	],
	"env": {
		"test": {
			"presets": [
				[
					"@babel/preset-env",
					{
						"targets": "current node"
					}
				]
			]
		}
	}
}
